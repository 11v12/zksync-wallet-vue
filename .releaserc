{
  "debug": false,
  "tagFormat": "${version}",
  "addReleases": "top",
  "npmPublish": false,
  "branches": [
    "+([0-9])?(.{+([0-9]),x}).x",
    "master",
    "next",
    {
      "name": "beta",
      "prerelease": true
    },
    {
      "name": "testnet",
      "prerelease": true
    }
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
    [
      "@semantic-release/npm",
      {
        "npmPublish": false
      }
    ],
    [
      "@semantic-release/github",
      {
        "assets": []
      }
    ]
  ]
}
