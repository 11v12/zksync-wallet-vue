{
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": "./src",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "inlineSources": true,
    "lib": [
      "esnext",
      "esnext.AsyncIterable",
      "dom"
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "paths": {
      "~/*": [
        "./*"
      ],
      "@/*": [
        "./*"
      ]
    },
    "resolveJsonModule": true,
    "sourceMap": true,
    "strict": true,
    "target": "esnext",
    "types": [
      "@nuxt/vue-app",
      "@nuxt/types",
      "@types/node",
      "@nuxt/types",
      "@types/jest",
      "@nuxtjs/axios",
      "@nuxtjs/sentry",
      "@nuxtjs/toast",
      "@inkline/inkline/types",
      "nuxt-typed-vuex",
      "nuxt-i18n"
    ]
  },
  "exclude": [
    "node_modules",
    "dist",
    ".yarn",
    "public",
    ".nuxt",
    ".github",
    "environments",
    "tests"
  ]
}
