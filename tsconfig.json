{
  "compilerOptions": {
    "target": "ESNext",
    "jsx": "preserve",
    "module": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "lib": [
      "dom",
      "ESNext",
      "ESNext.AsyncIterable"
    ],
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "baseUrl": "./src",
    "paths": {
      "~/*": [
        "./*"
      ],
      "@/*": [
        "./*"
      ],
      "~~/*": [
        "./*"
      ],
      "@@/*": [
        "./*"
      ]
    },
    "types": [
      "vue-meta",
      "@types/node",
      "@types/jest",
      "@nuxt/types",
      "@nuxtjs/axios",
      "@nuxt/vue-app",
      "lodash",
      "@nuxtjs/eslint-config-typescript",
      "@nuxtjs/axios",
      "@nuxtjs/style-resources",
      "@inkline/inkline",
      "@inkline/nuxt",
      "@nuxtjs/sentry",
      "@nuxtjs/toast",
      "nuxt-i18n",
      "@types/node",
      "@nuxt/types"
    ]
  },
  "exclude": [
    "node_modules",
    ".yarn",
    "dist",
    "public",
    "cypress",
    "src/blocks/Transaction",
    "tests",
    ".github",
    "environments"
  ]
}
