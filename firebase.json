{
  "hosting": [
    {
      "public": "build",
      "target": "ropsten-beta",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "headers": [
        {
          "source": "/**",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "no-cache, no-store, must-revalidate"
            }
          ]
        },
        {
          "source": "assets/**",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "max-age=14400"
            }
          ]
        },
        {
          "source": "**/*.@(js|wasm)",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "max-age=14400"
            }
          ]
        },
        {
          "source": "**/*.@(css)",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "max-age=14400"
            }
          ]
        },
        {
          "source": "**/*.@(eot|otf|ttf|ttc|woff|font.css)",
          "headers": [
            {
              "key": "Access-Control-Allow-Origin",
              "value": "*"
            },
            {
              "key": "Cache-Control",
              "value": "max-age=14400"
            }
          ]
        },
        {
          "source": "**/*.@(jpg|jpeg|gif|png)",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "max-age=14400"
            }
          ]
        },
        {
          "source": "*.@(jpg|jpeg|gif|png)",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "no-cache"
            }
          ]
        },
        {
          "source": "/sw.js",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "no-cache"
            }
          ]
        }
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    },
