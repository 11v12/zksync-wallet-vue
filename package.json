{
  "name": "zksync-dapp-wallet",
  "author": "Matter Labs",
  "private": true,
  "main": "nuxt.config.ts",
  "types": "src/types/index.d.ts",
  "files": [
    "src/**/*",
    "tests/*"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "version": "2.3.9",
  "homepage": "https://wallet.zksync.io",
  "description": "zkWallet was created to unleash the power of zkSync L2 operations and give anyone an access to L2 zkSync features on mainnet",
  "bugs": {
    "url": "https://github.com/matter-labs/zksync-wallet-vue/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint --format -E HUSKY_GIT_PARAMS"
    }
  },
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://www.opensource.org/licenses/mit-license.php"
    },
    {
      "type": "Apache-2.0",
      "url": "https://opensource.org/licenses/apache2.0.php"
    }
  ],
  "lint-staged": {
    "*.{css,scss}": "yarn run lint-css",
    "*.{ts,tsx}": [
      "yarn run lint-ts",
      "yarn run format"
    ]
  },
  "repository": "https://github.com/matter-labs/zksync-wallet-vue",
  "scripts": {
    "ci:clear": "bash cli-dev.sh ci",
    "ci:build": "yarn run ci && bash ./cli-process-env.sh \"$NETWORK\" && yarn generate --fail-on-error",
    "ci:prepare:rinkeby": "sh cli-process-env.sh 'rinkeby' dev 1",
    "ci:prepare:ropsten": "sh cli-process-env.sh 'ropsten' dev 1",
    "ci:prepare:mainnet": "sh cli-process-env.sh 'mainnet' dev 1",
    "ci:validate": "yarn ci && sh cli-process-env.sh 'mainnet' dev 1",
    "deploy:prod": "./cli-deploy-production.sh",
    "dev": "sh cli-process-env.sh 'rinkeby' 'dev' 1 1 && nuxt",
    "dev-beta": "sh cli-process-env.sh 'rinkeby-beta' 'dev' 1 1 && nuxt",
    "prepare_ci": "yarn ci && sh cli-process-env.sh 'rinkeby'",
    "start": "sh cli-process-env.sh 'rinkeby' 'dev' && nuxt start",
    "build": "sh cli-process-env.sh 'mainnet' 'prod' true && nuxt build",
    "build:stage": "yarn ci && sh cli-process-env.sh 'rinkeby' 'dev' && yarn generate --fail-on-error",
    "generate": "nuxt generate",
    "test": "jest",
    "fmt:check": "npx prettier --config .prettierrc.js --ignore-path .gitignore -c src/",
    "lint-ts:check": "eslint -c tsconfig.eslint.json -c .eslintrc.js --fix-dry-run --ext \".js,.ts,.vue\" src/**",
    "lint-style:check": "stylelint --config .stylelintrc  --ignore-path .stylelintrc src/",
    "lint-ts:fix": "eslint -c tsconfig.eslint.json -c .eslintrc.js --ext \".js,.ts,.vue\" --fix src/",
    "lint-style:fix": "stylelint --config .stylelintrc --ignore-path .stylelintrc src/"
  },
  "dependencies": {
    "@matterlabs/zk-wallet-onboarding": "1.18.6",
    "@nuxt/components": "2.1.6",
    "@nuxt/http": "^0.6.4",
    "@nuxt/typescript-runtime": "latest",
    "@nuxtjs/axios": "^5.13.1",
    "@nuxtjs/google-gtag": "^1.0.4",
    "@nuxtjs/sentry": "latest",
    "@nuxtjs/toast": "^3.3.1",
    "ethereum-blockies-base64": "^1.0.2",
    "ethers": "^5.1.0",
    "lottie-web": "^5.7.8",
    "moment-timezone": "^0.5.33",
    "nuxt": "latest",
    "nuxt-build-optimisations": "^1.0.3",
    "nuxt-typed-vuex": "latest",
    "vue": "latest",
    "vue-qrcode": "^0.3.5",
    "vue-scrollto": "^2.20.0",
    "web3": "^1.3.5",
    "zksync": "0.10.9"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/runtime": "^7.13.10",
    "@ethersproject/logger": "^5.1.0",
    "@ethersproject/providers": "^5.1.0",
    "@inkline/nuxt": "^2.3.6",
    "@nuxt/types": "latest",
    "@nuxt/typescript": "latest",
    "@nuxt/typescript-build": "latest",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/eslint-config-typescript": "^6.0.0",
    "@nuxtjs/google-fonts": "^1.3.0",
    "@nuxtjs/style-resources": "latest",
    "@types/jest": "^26.0.22",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "fork-ts-checker-webpack-plugin": "^6.2.0",
    "jest": "^26.6.3",
    "node-sass": "4.14.0",
    "prettier": "^2.2.1",
    "qrcode": "^1.0.0",
    "sass-loader": "10.1.1",
    "stylelint": "^13.12.0",
    "stylelint-config-recommended": "^4.0.0",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-scss": "^3.19.0",
    "typed-vuex": "latest",
    "typescript": "^4.2.4",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2",
    "vuex": "3.5.1",
    "web3-core": "^1.3.5",
    "webpack": "4.46.0"
  },
  "peerDependencies": {
    "@babel/core": "*",
    "@babel/runtime": "*",
    "typed-vuex": "*",
    "vuex": "3.5.1"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "fork-ts-checker": {
    "typescript": true
  },
  "optionalDependencies": {
    "fs-extra": "^9.1.0",
    "fsevents": "^2.3.2"
  }
}
