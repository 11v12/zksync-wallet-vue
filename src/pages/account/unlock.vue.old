<template>
  <div class="transactionPage">
    <account-unlock />
  </div>
</template>

<script lang="ts">
import Vue from 'vue';
import AccountUnlock from "@/blocks/AccountUnlock.vue";
export default Vue.extend({
  components: {
    AccountUnlock,
  },
  asyncData({ from, redirect, store }) {
    if (!store.getters["wallet/isAccountLocked"]) {
      redirect("/account");
    }
    return {
      fromRoute: from,
    };
  },
});
</script>
